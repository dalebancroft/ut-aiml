        "print(y.value_counts(1))\n",
        "print(\"*\" * 80)\n",
        "print(\"Target value ratio in y_train\")\n",
        "print(y_train.value_counts(1))\n",
        "print(\"*\" * 80)\n",
        "print(\"Target value ratio in y_val\")\n",
        "print(y_val.value_counts(1))\n",
        "print(\"*\" * 80)\n",
