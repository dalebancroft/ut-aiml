      "source": [
        "# Creating a set of models and evaluating their performance on training and validation data sets\n",
        "\n",
        "# Empty list to store all the models\n",
        "models = []\n",
        "\n",
        "# Appending models into the list\n",
        "models.append((\"dtree\", DecisionTreeClassifier(random_state=1, class_weight='balanced')))\n",
        "models.append((\"Bagging\", BaggingClassifier(base_estimator=DecisionTreeClassifier(random_state=1, class_weight='balanced'), random_state=1)))\n",
        "models.append((\"Random Forest\", RandomForestClassifier(random_state=1, class_weight='balanced')))\n",
        "models.append((\"Adaboost\", AdaBoostClassifier(random_state=1)))\n",
        "models.append((\"GradientBoosting\", GradientBoostingClassifier(random_state=1)))\n",
        "models.append((\"XGBoost\", XGBClassifier(random_state=1, verbosity = 0)))\n",
        "\n",
        "# Empty list to store model performance results\n",
        "model_results_1 = []\n",
        "\n",
        "# Train the models on the unmodified training data and add Recall score to list\n",
        "for name, model in models:\n",
        "    model.fit(X_train, y_train)\n",
        "    scores_train = recall_score(y_train, model.predict(X_train))\n",
        "    model_results_1.append({'Model Name': name, 'Train Recall': scores_train})\n",
        "\n",
        "# Train the models on the unmodified validation data and add Recall score to list\n",
        "for i, (name, model) in enumerate(models):\n",
        "    model.fit(X_train, y_train)\n",
        "    scores_val = recall_score(y_val, model.predict(X_val))\n",
        "    model_results_1[i]['Val Recall'] = scores_val\n",
        "\n",
        "# Output Recall performance of all models against unmodified training and validation data sets\n",
        "print(\"\\n\" \"Model performance - unmodified data\" \"\\n\")\n",
        "df_model_results_1 = pd.DataFrame(model_results_1)\n",
        "df_model_results_1"
      ]
