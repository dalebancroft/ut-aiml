      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Checking that no categorical variable has missing or nonsensical values in the validation data set\n",
        "cols = X_val.select_dtypes(include=['object','category']).columns.tolist()\n",
        "\n",
        "for field in cols:\n",
        "  print(X_val[field].value_counts(dropna=False)) # Use np.nan to represent NaN values\n",
        "  print(\"*\"*50)"
      ],
