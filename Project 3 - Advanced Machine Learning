        "# Create a single dataframe listing the validation performance of all models on all data sets\n",
        "merged_df = pd.merge(df_model_results_1[['Model Name', 'Val Recall']],\n",
        "                     df_model_results_2[['Model Name', 'Val Recall']],\n",
        "                     on='Model Name', suffixes=('_Original', '_Oversampled')) # Changed 'columns' to 'suffixes' and modified values\n",
        "merged_df = pd.merge(merged_df,\n",
        "                     df_model_results_3[['Model Name', 'Val Recall']],\n",
        "                     on='Model Name', suffixes=('', '_undersampled'))\n",
        "merged_df = merged_df.rename(columns={'Val Recall': 'Val Recall_undersampled'})\n",
